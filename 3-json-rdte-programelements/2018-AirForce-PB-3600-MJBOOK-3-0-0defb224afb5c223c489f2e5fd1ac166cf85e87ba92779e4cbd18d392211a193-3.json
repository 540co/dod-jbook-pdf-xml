                            "AccomplishmentPlannedProgram": [
                                {
                                    "Title": {
                                        "val": "Cross Domain OPIR"
                                    },
                                    "Description": {
                                        "val": "Cross Domain OPIR delivers a mechanism providing reach-back support for battlespace awareness functions and manages the timely transfer of newly developed tools to operational users; specifically by developing tools & tradecraft on a flexible architecture allowing easy integration of new capabilities into air component operations and by providing advanced \u201cstate of art\u201d algorithms and tools for the warfighter"
                                    },
