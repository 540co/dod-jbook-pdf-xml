                        },
                        "AccomplishmentPlannedProgramList": {
                            "AccomplishmentPlannedProgram": [
                                {
                                    "Title": {
                                        "val": "Anti-Tamper (AT) Technology Support"
                                    },
                                    "Description": {
                                        "val": "AT is a DoD program that encompasses the systems engineering activities intended to prevent and\/or delay exploitation of critical technologies in U.S. weapon systems. These activities involve the entire life-cycle of systems acquisition, including research, development, implementation, and testing of AT measures."
                                    },
                                    "Accomplishment": {
                                        "PriorYear": {
                                            "Funding": {
                                                "val": "4.158"
                                            },
                                            "Text": {
                                                "val": "Continued to build and maintain the PT core team of subject matter experts (SMEs) to support the development of over 30 Army programs and evaluation of their AT architectures. In support of that primary mission, PT continued to build and maintain its state-of-the-art RE capabilities to facilitate technical assessments to evaluate the vulnerabilities of micro-electronic components used in the electronic designs of Army weapons systems with critical program information (CPI) that required protection."
                                            }
                                        }
                                    },
                                    "PlannedProgram": {
                                        "CurrentYear": {
                                            "Funding": {
                                                "val": "8.105"
                                            },
                                            "Text": {
                                                "val": "Will continue to build and maintain the PT core team of subject matter experts (SMEs) available for this ongoing mission to support the development of Army programs and evaluating their AT architectures.  In support of that primary mission, PT must and will continue to build and maintain state-of-the-art RE capabilities to facilitate technical assessments to evaluate the vulnerabilities of micro-electronic components used in the electronic designs of Army weapons systems with critical program information (CPI) that requires protection."
                                            }
                                        },
                                        "BudgetYearOne": {
                                            "Funding": {
                                                "val": "8.682"
                                            }
                                        },
                                        "BudgetYearOneBase": {
                                            "Funding": {
                                                "val": "8.682"
                                            },
                                            "Text": {
                                                "val": "Will continue to build and maintain the PT core team of SMEs available for this ongoing mission to support the development of Army programs and evaluating their AT architectures. In support of that primary mission, PT must and will continue to build and maintain state-of-the-art RE capabilities to facilitate technical assessments to evaluate the vulnerabilities of micro-electronic components used in the electronic designs of Army weapons systems with CPI that requires protection."
                                            }
                                        },
                                        "AppChangeSummary": {
                                            "val": "FY 2021 increase supports growing number of Army programs requiring AT support and technical oversight"
                                        }
